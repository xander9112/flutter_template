{
    "explorer.fileNesting.patterns": {
        "mason.yaml": "mason-lock.json",
        "pubspec.yaml": ".packages, .metadata, pubspec.lock, analysis_options.yaml, .flutter-plugins, .flutter-plugins-dependencies, xela_app.iml, melos_xela_app.iml",
        "flutter_launcher_icons.yaml": "flutter_launcher_icons-*.yaml",
        "*.dart": "${capture}.g.dart, ${capture}.freezed.dart",
        "melos.yaml": "melos_*.iml"
    },
    "java.configuration.updateBuildConfiguration": "automatic",
    "cSpell.words": [
        "appiconset",
        "canonicalized",
        "dartz",
        "Demovich",
        "dotenv",
        "Encrypter",
        "Flavorizr",
        "fullscreen",
        "phonebook",
        "prefs",
        "regexes",
        "Subtags",
        "unawaited",
        "xcassets"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.formatOnSave": true,
    "java.compile.nullAnalysis.mode": "disabled"
}